<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
    <meta>
        <file-type type="string">FlightGear add-on menu bar items</file-type>
        <format-version type="int">1</format-version>
    </meta>
    <menubar-items>
        <menu>
            <label>AnotherGUI</label>
            <enabled type="bool">true</enabled>
            <item>
                <label>Select GUI style</label>
                <binding>
                    <command>dialog-show</command>
                    <dialog-name>AnotherGUIStyle</dialog-name>
                </binding>
            </item>
 
            <item>
                <label>Reload</label>
                <binding>
                    <command>addon-reload</command>
                    <id>sp.ntx.AnotherGUI</id>
                </binding>
            </item> 
            
        </menu>
    </menubar-items>
</PropertyList>
